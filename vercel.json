{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ],
{
  "rewrites": [
    {
      "source": "/:folder/:subfolder",
      "destination": "/:folder/:subfolder/:subfolder.html"
    }
  ]
}
